  "bugs": {
    "url": "https://github.com/griffinsharp/immortalkombat/issues"
  },
  "homepage": "https://github.com/griffinsharp/immortalkombat#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "concurrently": "^5.0.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.7.11",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "react-qr-reader": "^2.2.1",
    "react-repeatable": "^2.0.1",
    "socket.io": "^2.4.0",
    "socket.io-client": "^2.3.0",
